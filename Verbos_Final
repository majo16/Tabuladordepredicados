        verbos_pasado_lista.append("compitió")
        
    elif i == "lucir":
        verbos_pasado_lista.append("lució")
        
    elif i[-4:len(i)] == "eñir":
        x = i[:-4] + "iñó"
        verbos_pasado_lista.append(x)
        
    elif i[-3:len(i)] == "ñir":
        x = i[:-2] + "ó"
        verbos_pasado_lista.append(x) 
        
    elif i[-4:len(i)] == "ucir":
        x = i[:-3] + "jo"
        verbos_pasado_lista.append(x) 

    elif i[-1] == "r" and i[-2] == "a":
        x = i[:l-2] + "ó"
        verbos_pasado_lista.append(x)
        
    elif i[-1] == "r" and i[-2] == "e":
        x = i[:l-2] +"ió"
        verbos_pasado_lista.append(x)
        
    elif (i[-1] == "r" and i[-2] == "i") or (i[-1] == "r" and i[-2] == "i" and i[-3] == "e"):
        x = i[:l-2] + "ió"
        verbos_pasado_lista.append(x)
        
    else:
        verbos_pasado_lista.append("N/A")

