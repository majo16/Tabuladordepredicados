    elif i == "aliviar":
        verbos_presente_lista.append("alivia")
        
    elif i == "agriar":
        verbos_presente_lista.append("agria")
        
    elif i == "adherir":
        verbos_presente_lista.append("adhiere")
        
    elif i == "acariciar":
        verbos_presente_lista.append("acaricia")
        
    elif i == "abreviar":
        verbos_presente_lista.append("abrevia")
        
    elif i[-3:l] == "iar":
        x = i[:-3] + "ía"
        verbos_presente_lista.append(x)    
        
    elif i[-3:l] == "uar":
        x = i[:-3] + "úa"
        verbos_presente_lista.append(x)
        
    elif i[-2:l] == "ar":
        x = i[:-2] + "a"
        verbos_presente_lista.append(x)
        
    elif i[-2:l] == "er":
        x = i[:-2] + "e"
        verbos_presente_lista.append(x)
    
    elif i[-2:l] == "ir":
        x = i[:-2] + "e"
        verbos_presente_lista.append(x)
        
    else:
        verbos_presente_lista.append("N/A")
 
tupla_verbos = tuple( zip (verbos_infinitivo_lista, verbos_futuro_lista, verbos_pasado_lista, verbos_presente_lista))
 
print(tupla_verbos)

def buscar_infinitivo(verbo):
    for i in tupla_verbos:
        for a in i:
            if a == verbo:
                print(i[0])

buscar_infinitivo("abolió")
