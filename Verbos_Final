verbos_futuro_lista =[]

for i in verbos_sin_espacio:
    l = len(i)
    
    if i == "caber":
        verbos_futuro_lista.append("cabrá" )
        
    elif i == "decir":
        verbos_futuro_lista.append("dirá" )
        
    elif i == "haber":
        verbos_futuro_lista.append("habrá" )
        
    elif i == "poder":
        verbos_futuro_lista.append("podrá" )
        
    elif i == "poner":
        verbos_futuro_lista.append("pondrá" )
        
    elif i == "querer":
        verbos_futuro_lista.append("querrá" )
        
    elif i == "saber":
        verbos_futuro_lista.append("sabrá" )
        
    elif i == "salir":
        verbos_futuro_lista.append("saldrá" )
        
    elif i == "tener":
        verbos_futuro_lista.append("tendrá" )
        
    elif i == "valer":
        verbos_futuro_lista.append("valdrá" )
        
    elif i == "venir":
        verbos_futuro_lista.append("vendrá" )
        
    elif i == "oponer":
        verbos_futuro_lista.append("opondrá")
    
    elif i[-1]== "r" and (i[-2] == "a" or "e" or "i"):
        x = i[:-1] + "rá"
        verbos_futuro_lista.append(x)
        
    else:
        verbos_futuro_lista.append("N/A")
        
