v = verbs.replace(" ", "")
#print(v)

cad = "ella dijo que abandonar abolió abjuró ablandó no iba a ir. Sin embargo, ella tiñó su pantalon mientras él le condujo. hasta su casa. Minutos despues logró conseguir lo que le faltaba."
patron2 = re.compile(v)
matches2 = patron2.finditer(cad)

for n in matches2:
    print(n)
